{
    "name": "pku-web-client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    
    "scripts": {
        "dev": "vite --host 0.0.0.0 --port 9999 ",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint --fix src/**/*.{ts,tsx}",
        "format": "prettier --write 'src/**/*.{css,ts,tsx,json}'"
    },
    "files": [
        "umd/*.js",
        "umd/*.map",
        "audio/*.m4a",
        "css/base.css",
        "css/base.min.css",
        "img/*.png",
        "img/*.svg",
        "img/*.jpeg",
        "index-dev.html",
        "index.html",
        "manifest.json",
        "service-worker.js",
        "firebase-init.js",
        "version.js"
