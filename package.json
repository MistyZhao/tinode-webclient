        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "postcss": "^8.4.35",
        "prettier": "3.2.5",
        "sass": "^1.58.3",
        "tailwindcss": "^3.4.1",
        "typescript": "^4.9.3",
