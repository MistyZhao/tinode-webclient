    "dependencies": {
        "@ant-design/icons": "^5.3.1",
        "@reduxjs/toolkit": "^1.9.3",
        "antd": "^5.15.2",
        "antd-img-crop": "^4.12.2",
        "axios": "^0.21.1",
        "cropperjs": "^1.6.1",
        "firebase": "^10.11.1",
        "http-proxy-middleware": "^3.0.0",
        "jsencrypt": "^3.2.0",
        "jsonp": "^0.2.1",
        "libphonenumber-js": "^1.10.61",
        "qrcodejs": "^1.0.0",
        "react": "^18.2.0",
        "react-cropper": "^2.3.3",
        "react-dom": "^18.2.0",
        "react-infinite-scroll-component": "^6.1.0",
        "react-intl": "^6.6.5",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.8.1",
        "redux": "^4.2.1",
        "redux-persist": "^6.0.0",
        "reset-css": "^5.0.1",
        "terser": "^5.19.4",
        "tinode-sdk": "^0.22.13",
        "ts-md5": "^1.3.1",
        "webm-duration-fix": "^1.0.4"
    },
